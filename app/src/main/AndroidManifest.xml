<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.restaurant.thariq">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.restaurant.thariq.CreateRestaurantActivity"></activity>
        <activity
            android:name="com.restaurant.thariq.ProfileActivity"
            android:label="@string/profile_account" />
        <activity android:name="com.restaurant.thariq.auth.ChangePasswordActivity" />
        <activity android:name="com.restaurant.thariq.auth.RegisterActivity" />
        <activity android:name="com.restaurant.thariq.auth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.restaurant.thariq.DashboardActivity" />
    </application>

</manifest>